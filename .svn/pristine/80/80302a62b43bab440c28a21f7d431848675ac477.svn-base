#mytel{
  z-index:999999999999;
  position:fixed;
  top:20%;
  right:90px;
  width:50px;
  height:50px;
  border-radius:100px;
  border:0px;
  background-color: #4f7d6a;
    -webkit-transition: all 1s; /* Safari */
}
#mytel:hover{
   -ms-transform: rotate(360deg); 
    -webkit-transform: rotate(360deg); 
    transform: rotate(360deg); 
