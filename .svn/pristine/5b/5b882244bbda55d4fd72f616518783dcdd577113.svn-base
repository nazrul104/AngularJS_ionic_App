@import url(http://fonts.googleapis.com/css?family=Raleway:700,400);
*{padding: 0;margin: 0;}
body {
  background-color: #ccc !important;
  color: #333;
  font-family: "Raleway","Helvetica Neue",Helvetica,Arial,sans-serif;
  font-size: 14px;  
  background-size: cover;
}
.wapper{
	width: auto;
}
img.logo{
    margin-top: 8px;
}
.navbar-wrapper {
  background: rgba(0, 0, 0, 0.7) none no-repeat scroll 0 0;
  border-color: #080808;
}
/*---navigation---------*/
.logo{
	text-align:center;
	}
/*-----middle content-----*/
.middle-content{
	background:#fff;
	border:1px solid #ccc;
	margin:10px 0;
	}
/*-----footer----*/
.footer-box{
    margin-top: 20px;
    background: rgba(0, 0, 0, 0.8) none no-repeat scroll 0 0;
    border-color: #080808;
    text-align: center;
    position: relative;
    bottom: 0;
    width: 100%;
}
.icon-smedia{min-height: 60px;}
.icon-smedia a {
  color: #ed193a;
  display: inline-block;
  font-size: 32px;
  margin: 8px 0;
  outline: medium none;
}
.middle-new-box {
  margin-bottom: 70px;
  overflow: hidden;
}
.sortby-box{
    background: #fff;
    margin: 0 14px;
    padding: 6px 0;
    border-radius: 4px;
}
.restaurant-filter-list{
    width: auto;
    margin-top: 10px;
}
.showing-results p{
    margin: 0;
    line-height: 27px;
}
.offer_available > p {
  font-weight: bold;
  line-height: 42px;
  text-align: right;
}
.menu-search-box{
    background: #fff;
    border-bottom: 1px solid #ccc;
}
.food-search{
    margin: 4px 0;
}
.food-search h3 {
  font-size: 18px;
  font-weight: bold;
  margin: 0;
  padding: 4px 0;
  text-align: left;
}
.food-search li{
    width: 40%;
}
.food-search li+li{
    width: 42%;
}
.food-search li+li+li{
    width: 16%;
}
.hungry-food-search{
    width: 100%;
}
.search-food-box{
    padding:10px 0;
}
/*------Menu------*/
.breadcrumb{
    margin: 8px 0;
}
.single-restaurant{
    margin: 0 15px;
}
.view_menu_btn {
  background-color: #21610B;
  border-radius: 4px;
  color: #fff;
  display: block;
  float: right;
  font-family: Bold,Verdana,sans-serif;
  font-size: 15px;
  height: 40px;
  line-height: 41px;
  text-align: center;
  text-transform: uppercase;
  width: 165px;
  border: none;
}
.view_menu_btn:hover {
    background: #0A8423;
    text-decoration: none;
    color: #fff;
}
.pre_order_btn {
  background-color: #DF0101;
  border-radius: 4px;
  color: #fff;
  display: block;
  float: right;
  font-family: Bold,Verdana,sans-serif;
  font-size: 15px;
  height: 40px;
  line-height: 41px;
  text-align: center;
  text-transform: uppercase;
  width: 165px;
  border: none;
}
.pre_order_btn:hover {
  background-color: #C11128;
  text-decoration: none;
  color:#fff;
}
.padding-l-r{
    padding: 0;
}
.tab-menu-box{
    width: 100%;
    overflow: hidden;
    margin-top: 8px;
    margin-bottom: 0;
}
.tab-menu-box li {
  margin-left: 5px;
  background: #fff none repeat scroll 0 0;
  padding: 0;
}
.tab-menu-box li+li {
  margin-left: 0px;
  background: #b7b5ba;
  padding: 0;
  color: #fff;
}
.tab-menu-box li+li+li {
  margin-left: 0px;
  background: #b7b5ba;
  padding: 0;

}
.tab-menu-box li p{
    margin: 0;
}
.tab-menu-box li p a {
  line-height: 40px;
  padding: 12px 18px;
    color: #444;
}
.tab-menu-box li p a:hover{
    text-decoration: none;
}
.menu-catagory-box{
    background: #fff;
}
.menu-catagory-list li {
  border-bottom: 1px solid #ccc;
  padding: 10px 0;
}
.menu-catagory-list li span{
    color: #444;
}
.menu-catagory-list li:hover {
  background: #ccc;
}
/*------My Account-----------*/
.myaccount{
    background: #fff;
    margin:20px 0;
    min-height: 500px;
}
.myaccount-bar-box{
    background: #ccc;
    margin: 8px 10px;
    border-radius: 4px;
}
.myaccount-ul{
    margin: 10px 0;
}
.online-order-btn{
    margin: 10px 0;
    display: block;
    text-align: center;
}
.myaccount-bottom-box{
}
.myaccont-list{
    width: 100%;
    border: none;
}
.myaccont-list li{
    clear: both;
    float:none;
    background: #ccc;
    margin: 2px 0;
    border-radius: 2px;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
}
.myaccont-list li a{
    color:#444;
    margin-right: 0;
    border: none;
}
.myaccont-list > li.active > a, .myaccont-list > li.active > a:focus, .myaccont-list > li.active > a:hover {
  -moz-border-bottom-colors: none;
  -moz-border-left-colors: none;
  -moz-border-right-colors: none;
  -moz-border-top-colors: none;
  background: #337ab7;
  border: none;
  color: #fff;
  cursor: default;
  margin: 0;
  border-radius: 2px;
}
.nav-tabs > li > a:hover {
  background: #337ab7;
  border-radius: 2px;
  color: #fff;
}
.tab-account-box{
  background: #ccc;
}
.box-account{
    margin: 6px 0;
}
.account-lbl {
  line-height: 36px;
  font-size: 12px;
  text-align: right;
}
.myaccount-control {
  background-color: #fff;
  background-image: none;
  border: 1px solid #ccc;
  border-radius: 4px;
  box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
  color: #555;
  display: block;
  font-size: 14px;
  height: 34px;
  line-height: 1.42857;
  padding: 6px 12px;
  transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
  border: 1px solid #b6b6b6;
}
.myaccount-control:focus {
  border-color: #66afe9;
  box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px rgba(102, 175, 233, 0.6);
  outline: 0 none;
}
.changePassword-box {
  float: none;
  margin: 26px auto;
  text-align: center;
}
.orderHistory-box{
    width: auto;
    clear: both;
    margin:10px 0;
    text-align: center;
}
.orderHistory-box table thead tr th{
    text-align: center;
}
/*----registration----*/
.registration-box{
    background: #fff;
    margin: 20px 0;
}
.checkout-box h3 {
  color: #444;
  font-size: 30px;
  font-weight: bold;
  margin: 28px 0;
  text-align: left;
}
.checkout-step{
    margin: 10px 0;
    padding: 10px 0;
    border: 1px solid #ccc;
    border-radius: 2px;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
}
.checkout-step li{
    margin: 0 6px;
}
.number-txt {
  color: #444;
  font-size: 48px;
  line-height: 20px;
  margin-top: 14px;
  text-align: left;
}
.checkout-txt{
    font-size: 16px;
    text-align: left;
    padding: 0 8px;
}
.number-txt-disable{
    color: #ccc;
  font-size: 48px;
  line-height: 20px;
  margin-top: 14px;
  text-align: left;
}
.checkout-txt-disable{
    font-size: 16px;
    text-align: left;
    padding: 0 8px;
    color: #ccc;
}
.registration-box-bottom h2{
    background: #83828A;
    padding: 12px 10px;
    margin: 0;
    font-size: 16px;
    color: #fff;
}
.padding-l{
    padding-left: 0;
}
.registration-bottom-inner{
    background: #EDEDEE;
    margin-bottom: 10px;
}
.register-inner{
    margin: 8px 0 6px;
}
.regter-lbl {
  color: #535353;
  font-size: 12px;
  line-height: 34px;
  text-align: left;
}
.dateofbirth{
    border: 1px solid #ccc;
    padding: 6px 0;
}
.agree-reg{
    margin: 0;
}